{
	"compilerOptions": {
		"baseUrl": ".",
		"target": "esnext",
		"useDefineForClassFields": true,
		"module": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"jsx": "preserve",
		"sourceMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"paths": {
			"~/*": ["src/*"],
			"@/*": ["src/*"]
		},
		"skipLibCheck": true,
		"lib": ["esnext", "dom", "dom.iterable", "scripthost"]
	},
	"vueCompilerOptions": {
		"target": 3
	}
}
